		if self.menu.menuconfig.start:Value() then
			Game.Chat.Print("<b><font color=\"#80c31b\">[PS] Ward Jump</font></b> <font color=\"#FFFFFF\">- Loaded successfully <u>v" .. self.version .. "</u>.")
		end
	end

	function PWJ:Update_Language()
		self.updateL = true
		self:Load_Menu()
		
		local number, string = self.menu.language.language:Value()
		local language = (({ [1] = "english", [2] = "german", [3] = "french", [4] = "dutch", [5] = "korean", [6] = "greek", [7] = "chinese", [8] = "portuguesebrazil", [9] = "turkish" })[tonumber(number)])
		
		self.menu:Name(self.menuT[1].pName[language])
	end

	function PWJ:Update_Default()
		self.updateD = true
		self:Load_Menu()
