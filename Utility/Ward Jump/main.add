class("objectManager")

	function objectManager:__init() --arocs minionmanager as base
		self.jumpobjects = {}
		
		Callback.Bind('CreateObj',function(obj) if myHero.charName == "JarvanIV" then self:AddFlag(obj) else self:AddMobs(obj) end end)
		Callback.Bind('CreateObj',function(obj) self:AddWard(obj) end)

		for i = 0,Game.ObjectCount() do
			local obj = Game.Object(i)
			if obj and obj.valid and obj.name then
				if myHero.charName == "JarvanIV" then
					self:AddFlag(obj)
				else
					self:AddMobs(obj)
					self:AddWard(obj)
				end
			end
		end

		for i = 0,Game.HeroCount() do
			local hero = Game.Hero(i)
			if hero and hero.valid and (myHero.charName == "LeeSin" and hero.team == TEAM_ALLY) or ((myHero.charName == "Katarina" or myHero.charName == "Jax") and (hero.team == TEAM_ALLY or hero.team == TEAM_ENEMY)) then
				 self:AddHero(hero)
			end
		end
	end

	function objectManager:AddMobs(obj)
		if obj.type == "obj_AI_Minion" and not obj.dead and (obj.charName:find('Minion') or obj.team == TEAM_NEUTRAL) and obj.health > 0 and (myHero.charName == "LeeSin" and obj.team ~= TEAM_ENEMY and obj.team ~= TEAM_NEUTRAL or myHero.charName == "Jax" or myHero.charName == "Katarina") then
			local minionFound = false
			for i=1,#self.jumpobjects do
				local minion = self.jumpobjects[i]
				if not minion or not minion.valid or not minion.health or minion.health == 0 or minion.dead then
					self.jumpobjects[i] = obj
					minionFound = true
					break
				end
			end

			if not minionFound then
				self.jumpobjects[#self.jumpobjects + 1] = obj
			end
		end
	end

	function objectManager:AddWard(obj)
		if (obj.name:find("Ward") or obj.name:find("Trinket")) and not obj.name:find("Idle") then
			local wardFound = false
			for i=1,#self.jumpobjects do
				local ward = self.jumpobjects[i]
				if not ward or not ward.valid or not ward.mana or ward.mana == 0 then
					self.jumpobjects[i] = obj
					wardFound = true
					break
				end
			end

			if not wardFound then
				self.jumpobjects[#self.jumpobjects + 1] = obj
			end
		end
	end

	function objectManager:AddFlag(obj)
		if obj.name and obj.name == "JarvanDemacianStandard_flag.troy" and obj.pos then
			local flagFound = false
			for i=1,#self.jumpobjects do
				local flag = self.jumpobjects[i]
				if not flag or not flag.valid then
					self.jumpobjects[i] = obj
					flagFound = true
					break
				end
			end

			if not flagFound then
				self.jumpobjects[#self.jumpobjects + 1] = obj
			end
		end
	end

	function objectManager:AddHero(hero)
		if not hero.dead and hero.health > 0 then
			local heroFound = false
			for i=1,#self.jumpobjects do
				local hero = self.jumpobjects[i]
				if not hero or not hero.valid or not hero.health or hero.health == 0 or hero.dead then
					self.jumpobjects[i] = hero
					heroFound = true
					break
				end
			end

			if not heroFound then
				self.jumpobjects[#self.jumpobjects + 1] = hero
			end
		end
	end

	function objectManager:GetJumpObjects(range, fromPos)
		self.objects = {}
		for i=1, #self.jumpobjects do
			local minion = self.jumpobjects[i]
			if minion and minion.valid and not minion.dead and minion.visible and minion.health > 0 and minion:DistanceTo(fromPos or myHero) < range then
				self.objects[#self.objects + 1] = minion
			end
		end
		return self.objects
	end

