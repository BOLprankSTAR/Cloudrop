	function PWJ:KeyCallback()
		if self.menu.keys.key:Value() and self.supportedSpells[myHero.charName].condition then
			self.keymode = "down" 
		else
			if self.cancel then 
				self.cancel 	= false
				self.keymode 	= nil
			else 
				self.keymode = "up"
			end
		end
	end

	----------------------------------------------------------------------

	function PWJ:Tick()
		self:Tick_Ready()
		self:Tick_Debug()
		self:Tick_GetJumpConditions()
		self:Tick_Cast()
	end

	function PWJ:Tick_Ready()
		if myHero.charName == "Jax" then
			self.supportedSpells.Jax.condition = myHero:CanUseSpell(self.supportedSpells.Jax.slot) == 0
		elseif myHero.charName == "Katarina" then
			self.supportedSpells.Katarina.condition = myHero:CanUseSpell(self.supportedSpells.Katarina.slot) == 0
		elseif myHero.charName == "LeeSin" then
			self.supportedSpells.LeeSin.condition = myHero:CanUseSpell(self.supportedSpells.LeeSin.slot) == 0 and myHero:GetSpellData(self.supportedSpells.LeeSin.slot).name == "BlindMonkWOne" and true or false
		elseif myHero.charName == "JarvanIV" then
			self.supportedSpells.JarvanIV.condition = myHero:CanUseSpell(self.supportedSpells.JarvanIV.slot) == 0
		end
		if not self.supportedSpells[myHero.charName].condition then
			self.keymode = nil
		end
	end

	function PWJ:Tick_Debug()
		if self.debugtime <= 255 and self.debugtime ~= 0 then
			self.debugtime = self.debugtime - 5
		end
	end

	function PWJ:Tick_GetJumpConditions()
		local number, string = self.menu.language.language:Value()
		local language = (({ [1] = "english", [2] = "german", [3] = "french" })[tonumber(number)])

		if myHero:DistanceTo(mousePos) <= self.wardrange-100 then
			self.jumppos = mousePos
		else
			self.jumppos = Geometry.Vector3(myHero.x, myHero.y, myHero.z) - (Geometry.Vector3(myHero.x, myHero.y, myHero.z) - mousePos):Normalize() * (self.wardrange-100)
		end

		if #wardjumpmanager:GetJumpObjects(self.wardrange, myHero) > 0 then
			for index, object in pairs(wardjumpmanager:GetJumpObjects(self.wardrange, myHero)) do
				local closestobj = GetClosestObject(mousePos, wardjumpmanager:GetJumpObjects(self.wardrange, myHero))
				if object.networkID == closestobj.networkID and closestobj:DistanceTo(self.jumppos) < self.menu.wardjump.scanradius:Value() then
					self.target = object
					if myHero.charName ~= "JarvanIV" then
						self.text = self.unit[language]
					end
				elseif object.networkID == closestobj.networkID and closestobj:DistanceTo(self.jumppos) > self.menu.wardjump.scanradius:Value() then
					self.target = nil
					if myHero.charName ~= "JarvanIV" then
						if self:GetBestWard(false, true) ~= nil then
							self.text = self:GetBestWard(false, true)
						else
							self.text = nil
						end
					end
				end
			end
		else
			self.target  = nil
			if myHero.charName ~= "JarvanIV" then
				if self:GetBestWard(false, true) ~= nil then
					self.text = self:GetBestWard(false, true)
				else
					self.text = "no possibilities found!"
				end
			end
		end
	end

	function PWJ:Tick_Cast()
		if self.keymode == "up" and self.supportedSpells[myHero.charName].condition then
			if self.target and self.jump == false then
				if myHero.charName == "JarvanIV" then
					myHero:CastSpell(self.supportedSpells[myHero.charName].slot, self.target)
				else
					myHero:CastSpell(self.supportedSpells[myHero.charName].slot, self.target)
				end
				self.keymode 	= nil
				self.jump 		= true
				self.keymode 	= nil
				self.target 	= nil
				DelayAction(function() self.jump = false end, .3)
			elseif myHero.charName == "JarvanIV" and myHero:CanUseSpell(2) == 0 then
				if self.ward == false then
					myHero:CastSpell(2, self.jumppos.x, self.jumppos.z)
					myHero:CastSpell(0, self.jumppos.x, self.jumppos.z)
					self.ward 	= true
					DelayAction(function() self.ward = false end, .3)
				end
			elseif myHero.charName ~= "JarvanIV" and self:GetBestWard(true, false) ~= nil then
				if self.ward == false then
					myHero:CastSpell(self:GetBestWard(true, false), self.jumppos.x, self.jumppos.z)
					self.ward 	= true
					DelayAction(function() self.ward = false end, .3)
				end
			else
				self.keymode = nil
				self.target = nil
			end
		end
	end

	function PWJ:GetLowestPriorityWard(x)
		for i = 1, 7 do
			if self:GetWardPriority(i) then
				return self:GetWardPriority(i)
			end
		end
	end

	function PWJ:GetBestWard(GetSlot, GetName)
		if self:GetLowestPriorityWard() ~= nil then
			local name, slot = self:GetLowestPriorityWard()
			if GetSlot then
				return slot
			elseif GetName then
				local number, string = self.menu.language.language:Value()
				local language = (({ [1] = "english", [2] = "german", [3] = "french" })[tonumber(number)])
				return name[language]
			end
		end
	end

	function PWJ:GetWardPriority(priority)
		for index, ward in pairs(self.wardTable) do
			for i=6, 12 do
				if myHero:GetSpellData(i).name == ward.name and myHero:CanUseSpell(i) == 0 and self.menu.priority[ward.name .. "Boolean"]:Value() and self.menu.priority[ward.name .. "Slider"]:Value() == priority then
					return ward.pName, i
				end
			end
		end
	end

	----------------------------------------------------------------------

	function PWJ:Draw()
		if not self.menu.graphics.enable:Value() then return end
		
		if self.supportedSpells[myHero.charName].condition and self.keymode == "down" then
			if self.menu.graphics.range.range:Value() then
				DrawCircle2(myHero, self.wardrange, self.menu.graphics.range.width:Value(), self.menu.graphics.range.color:Value(), self.menu.graphics.range.quality:Value(), self.menu.graphics.range.lfc:Value(), true)
			end

			if self.menu.graphics.scan.position:Value() then
				local color = Graphics.ARGB(255,255,255,255)
				if myHero:DistanceTo(mousePos) <= self.wardrange-80 then
					color = self.menu.graphics.scan.colorinrange:Value()
				else
					color = self.menu.graphics.scan.coloroutrange:Value()
				end
				DrawCircle2(mousePos, self.menu.wardjump.scanradius:Value(), self.menu.graphics.scan.width:Value(), color, self.menu.graphics.scan.quality:Value(), self.menu.graphics.scan.lfc:Value(), true)
			end

			if self.menu.graphics.wardpos.position:Value() then
				DrawCircle2(self.jumppos, 50, self.menu.graphics.wardpos.width:Value(), self.menu.graphics.wardpos.color:Value(), self.menu.graphics.wardpos.quality:Value(), self.menu.graphics.wardpos.lfc:Value(), true)
			end

			if self.menu.graphics.scan.unit:Value() and self.target ~= nil then
				DrawCircle2(self.target, 80, self.menu.graphics.scan.width:Value(), Graphics.ARGB(255,0,255,0), self.menu.graphics.scan.quality:Value(), self.menu.graphics.scan.lfc:Value(), true)
			end

			if myHero.charName ~= "JarvanIV" and self.menu.graphics.info.position:Value() == true and self.text then
				local pos = Graphics.WorldToScreen(Geometry.Vector3(self.jumppos.x, self.jumppos.y, self.jumppos.z))
				local color = self.menu.graphics.info.color:Value()
				Graphics.DrawText(self.text, self.menu.graphics.info.size:Value(), pos.x+tonumber(self.menu.graphics.info.abszisse:Value()), pos.y+tonumber(self.menu.graphics.info.ordinate:Value()), color)
			end
		end

		DrawCircle2(mousePos, self.menu.wardjump.scanradius:Value(), 2, Graphics.ARGB(self.debugtime,255,255,255), 24, true, true)
	end

	----------------------------------------------------------------------

	function PWJ:WndMsg(msg, key)
		if msg == 513 and self.keymode == "down" then
			self.cancel 	= true
			self.keymode 	= nil
			self.target 	= nil
		end
	end

